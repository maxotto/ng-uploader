  },
  "bugs": {
    "url": "https://github.com/trendster-io/ng-uploader/issues"
  },
  "keywords": [
    "Trendster",
    "Angular",
    "File Upload",
    "Interceptor",
    "HttpClient"
  ],
  "scripts": {
    "build": "ng build ng-uploader",
    "test": "ng test ng-uploader",
    "start-api": "node ./projects/ng-uploader-demo/api/index",
    "start-client": "ng serve ng-uploader-demo",
    "start-demo": "npm run start-api",
    "demo": "npm run build && npm run start-demo"
